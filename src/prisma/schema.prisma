// yarn prisma db push --schema=./src/prisma/schema.prisma
generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

model Link {
    hash        String     @id
    origin_uri  String
    price       Decimal
    
    @@index([hash,origin_uri])
}